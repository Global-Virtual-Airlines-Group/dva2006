<?xml version="1.0" encoding="ISO-8859-1"?>
<services name="default">
	<!-- RSS Feeds -->
	<service id="cooler_rss" class="org.deltava.service.rss.CoolerSyndicationService" />
	<service id="event_rss" class="org.deltava.service.rss.EventSyndicationService" />
	<service id="notams_rss" class="org.deltava.service.rss.NoticeSyndicationService" />
	<service id="news_rss" class="org.deltava.service.rss.NewsSyndicationService" />
	<service id="blog_rss" class="org.deltava.service.rss.BlogSyndicationService" />

	<!-- Fleet Library Services -->
	<service id="fleet_info" class="org.deltava.service.FleetInfoService" />
	<service id="fleet_sysinf" class="org.deltava.service.SystemInfoService" />
	<service id="fleetlib" class="org.deltava.service.InstallerInfoService" />

	<!-- Flight Schedule Services -->
	<service id="airports" class="org.deltava.service.schedule.AirportListService" />
	<service id="next_flight" class="org.deltava.service.schedule.AvailableFlightService" />
	<service id="next_leg" class="org.deltava.service.schedule.AvailableFlightLegService" />
	<service id="route" class="org.deltava.service.schedule.RouteMapService" />
	<service id="routeplot" class="org.deltava.service.schedule.RoutePlotMapService" />
	<service id="rmap_airports" class="org.deltava.service.schedule.ServicedAirportService" />
	<service id="rmap_routes" class="org.deltava.service.schedule.RouteService" />

	<!-- Pilot Data Services -->
	<service id="pilotmap" class="org.deltava.service.PilotLocationService" />
	<service id="onlinepilots" class="org.deltava.service.OnlinePilotService" />

	<service id="answer" class="org.deltava.service.ExamAnswerService" />

	<!-- ACARS/Google Maps Services -->
	<service id="acars_data" class="org.deltava.service.acars.FlightDataExportService" />
	<service id="acars_error" class="org.deltava.service.acars.ErrorLogService" />
	<service id="acars_map" class="org.deltava.service.acars.MapService" />
	<service id="acars_progress" class="org.deltava.service.acars.MapProgressService" />
	<service id="acars_pirep" class="org.deltava.service.acars.MapFlightDataService" />
	<service id="acars_earth" class="org.deltava.service.acars.FlightDataEarthService" />

	<!-- The names of ACARSMapEarthServices are hardcoded in ACARSMapEarthLinkService -->
	<service id="acars_map_earth" class="org.deltava.service.acars.EarthMapLinkService" />
	<service id="acars_map_eprog" class="org.deltava.service.acars.EarthMapService" />
	<service id="acars_map_eplan" class="org.deltava.service.acars.EarthMapPlanService" />

	<!-- ServInfo Services -->
	<service id="servinfo" class="org.deltava.service.servinfo.DataService" />
	<service id="si_route" class="org.deltava.service.servinfo.MapRouteService" />
</services>
