			<url>http://www.ivao.org/</url>
		</ivao>
		
		<fpi>
			<status_url>http://dva2006.deltava.org/fpi-data.txt</status_url>
			<banner>fpi.png</banner>
			<url>http://www.flightproject.net/</url>
		</fpi>
		
		<default_network>VATSIM</default_network>
	</online>
	
