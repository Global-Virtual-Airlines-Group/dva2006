	<!-- JDBC data source configuration -->
	<jdbc>
		<driver>com.mysql.jdbc.Driver</driver>
		<url>jdbc:mysql://mysql.deltava.org/dva</url>
		<user>dva</user>
		<pwd>***REMOVED***</pwd>
		
		<map name="connectProperties">
			<useNewIO>false</useNewIO>
			<autoReconnect>true</autoReconnect>
			<useCompression>true</useCompression>
		</map>
		
		<pool_size type="Integer">1</pool_size>
		<pool_max_size type="Integer">4</pool_max_size>
		<auto_commit type="Boolean">true</auto_commit>
	</jdbc>
	
	<html>
		<table>
			<spacing type="Integer">3</spacing>
			<padding type="Integer">3</padding>
			<viewSize type="Integer">60</viewSize>
		</table>
		
		<list name="schemes" attr="css">
			<css>legacy</css>
		</list>
	</html>
	
	<log>
		<list name="names" attr="name">
			<name>APP</name>
			<name>TASKS</name>
		</list>
		
		<purge type="Integer">10</purge>
	</log>
	
	<config>
		<commands>/etc/commands.xml</commands>
		<services>/etc/services.xml</services>
		<tasks>/etc/tasks.xml</tasks>
		<nat>http://www.natroutes.glideslope.de/html/nats.php3</nat>
	</config>
	
	<online>
		<list name="networks" attr="network">
			<network>VATSIM</network>
			<network>ACARS</network>
			<network>IVAO</network>
			<network>FPI</network>
		</list>
		
		<list name="highlightCodes" attr="code">
			<code>DAL</code>
			<code>DVA</code>
		</list>
		
		<vatsim>
			<status_url>http://usa-s1.vatsim.net/data/status.txt</status_url>
			<banner>vatsim.gif</banner>
			<url>http://www.vatsim.net/</url>
		</vatsim>
		
		<ivao>
			<status_url>http://www.ivao.org/whazzup/status.txt</status_url>
