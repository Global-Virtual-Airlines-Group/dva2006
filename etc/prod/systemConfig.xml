		<css>css</css>
		<js>js</js>
		<img>img</img>
		<library>/usr/local/www/library</library>
	</path>
	
	<smtp>
		<server>golgotha.deltava.org</server>
		<errors-to>webmaster@sce.net</errors-to>
		<testMode type="Boolean">true</testMode>
	</smtp>
	
	<registration>
		<list name="reject_domain" attr="domain">
			<domain>yahoo.com</domain>
			<domain>hotmail.com</domain>
		</list>
		
		<auto_reject type="Integer">5</auto_reject>
	</registration>
	
	<issue_track>
		<list name="versions" attr="version" sorted="true">
			<version>1.0</version>
			<version>1.01</version>
			<version>1.1</version>
		</list>
		
		<assignto type="Integer">043</assignto>
	</issue_track>
	
	<testing>
		<max_video_size type="Integer">3072000</max_video_size>
		<video_removal type="Integer">7</video_removal>
	</testing>
	
	<cooler>
