		<inactive_days type="Integer">90</inactive_days>
		<inactive_leave_days type="Integer">180</inactive_leave_days>
		<inactive_new_days type="Integer">21</inactive_new_days>
		<inactive_assign type="Integer">5</inactive_assign>
		
		<user_cookie_age type="Integer">30</user_cookie_age>
		
		<pirep>
			<maxDays type="Integer">1</maxDays>
			<minDays type="Integer">45</minDays>
			<pad_hours type="Double">0.5</pad_hours>
		</pirep>
		
		<!-- This controls whose user ID the Scheduled Tasks will operate as -->
		<tasks_by>Ralph Snyder</tasks_by>
	</users>
	
	<!-- ACARS Server Properties -->
	<acars>
		<enabled type="Boolean">true</enabled>
		<daemon>org.deltava.acars.TomcatDaemon</daemon>
		
		<livemap>
			<lat type="Double">38.88</lat>
			<lng type="Double">-93.25</lng>
			<reload type="Integer">15000</reload>
		</livemap>
		
		<port type="Integer">15527</port>
		<sleep type="Integer">1000</sleep>
		<timeout type="Integer">240</timeout>
		<position_interval type="Integer">2750</position_interval>
		
		<pool>
			<size type="Integer">100</size>
			<multiple type="Boolean">true</multiple>
		</pool>
		
		<buffer>
			<send type="Integer">10240</send>
			<recv type="Integer">1024</recv>
		</buffer>
		
		<list name="login_msgs" attr="msg">
			<msg>All messages are logged by user ID and IP address.</msg>
			<msg>Filed PIREPs will not count for logbook or promotion.</msg>
		</list>
		
		<ack>
			<ping type="Boolean">false</ping>
			<info type="Boolean">true</info>
			<position type="Boolean">true</position>
			<text type="Boolean">false</text>
		</ack>
	</acars>
